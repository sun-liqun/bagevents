<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="94px"
    android:background="@color/white"
    android:orientation="horizontal">

    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/ll_manager_center"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/img_manager_center"
            android:layout_width="17dp"
            android:layout_height="17dp"
            android:src="@drawable/tabbar_manage_unselected" />

        <TextView
            android:id="@+id/text_manager_center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8px"
            android:text="@string/text_manager_center"
            android:textColor="#484848"
            android:textSize="10sp" />
    </com.zhy.autolayout.AutoLinearLayout>

    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/ll_meeting_person"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/img_meeting_person"
            android:layout_width="19dp"
            android:layout_height="16dp"
            android:layout_marginTop="2dp"
            android:src="@drawable/tabbat_attendee_unselect" />

        <TextView
            android:id="@+id/text_meeting_person"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10px"
            android:text="@string/attendee"
            android:textColor="#484848"
            android:textSize="10sp" />
    </com.zhy.autolayout.AutoLinearLayout>

    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/ll_activity_order"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="visible">

        <ImageView
            android:id="@+id/img_activity_order"
            android:layout_width="14dp"
            android:layout_height="17dp"
            android:layout_marginTop="2dp"
            android:src="@drawable/tabbar_order_unselect" />

        <TextView
            android:id="@+id/text_activity_order"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8px"
            android:text="@string/text_activity_order"
            android:textColor="#484848"
            android:textSize="10sp" />
    </com.zhy.autolayout.AutoLinearLayout>

    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/ll_collect_manager_tab"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="visible">

        <ImageView
            android:id="@+id/img_collect_manager"
            android:layout_width="17dp"
            android:layout_height="17dp"
            android:layout_marginTop="2dp"
            android:src="@drawable/collect_click" />

        <TextView
            android:id="@+id/text_collect_manager"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8px"
            android:text="@string/text_collect_manager"
            android:textColor="#484848"
            android:textSize="10sp" />
    </com.zhy.autolayout.AutoLinearLayout>

    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/ll_exhibitor_manager"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="gone">

        <ImageView
            android:id="@+id/img_exhibitor_manager"
            android:layout_width="17dp"
            android:layout_height="17dp"
            android:layout_marginTop="2dp"
            android:src="@drawable/exhibitor_grey"/>

        <TextView
            android:id="@+id/text_exhibitor_manager"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8px"
            android:text="@string/text_exhibitor_manager"
            android:textColor="#484848"
            android:textSize="10sp" />
    </com.zhy.autolayout.AutoLinearLayout>


    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/ll_sign_in"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="visible">

        <ImageView
            android:id="@+id/img_sign_in"
            android:layout_width="19dp"
            android:layout_height="19dp"
            android:layout_marginTop="2px"
            android:src="@drawable/tab_more" />

        <TextView
            android:id="@+id/text_sign_in"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6px"
            android:layout_marginBottom="2px"
            android:text="@string/more"
            android:textColor="#484848"
            android:textSize="10sp" />
    </com.zhy.autolayout.AutoLinearLayout>


    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/ll_barcode"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="gone">

        <ImageView
            android:id="@+id/img_barcode"
            android:layout_width="19dp"
            android:layout_height="19dp"
            android:layout_marginTop="2px"
            android:src="@drawable/erweima_bottom" />

        <TextView
            android:id="@+id/text_barcode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6px"
            android:layout_marginBottom="2px"
            android:text="@string/checkin"
            android:textColor="#484848"
            android:textSize="10sp" />
    </com.zhy.autolayout.AutoLinearLayout>

</com.zhy.autolayout.AutoLinearLayout>